{
  "compilerOptions": {
			"jsx": "react",
      "strict": false,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES2022",
      "moduleResolution": "Node10",
      "module": "CommonJS",
      "declaration": true,
      "isolatedModules": true,
      "noEmit": true,
      "outDir": "dist"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}